<?xml version="1.0"?>
<facelet-taglib version="2.2"
                xmlns="http://xmlns.jcp.org/xml/ns/javaee"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd">

    <namespace>http://primefaces.org/ui/extensions/showcase</namespace>

    <function>
        <function-name>getFileContent</function-name>
        <function-class>org.primefaces.extensions.showcase.util.ShowcaseUtil</function-class>
        <function-signature>java.lang.String getFileContent(java.lang.String)</function-signature>
    </function>

    <tag>
        <tag-name>convertOrder</tag-name>
        <converter>
            <converter-id>org.primefaces.extensions.showcase.converter.OrderConverter</converter-id>
        </converter>
        <attribute>
            <name>events</name>
            <required>true</required>
            <type>java.util.List</type>
        </attribute>
    </tag>

</facelet-taglib>
